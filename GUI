Add-ADGroupMember -Identity GLS-U-REV-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Tom Price")
{

Add-ADGroupMember -Identity 291296ad-b426-4686-b4c0-fdcc293979c1 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-PER-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Townsville")
{

Add-ADGroupMember -Identity ec27144a-0f98-4fce-bf18-f244618d2be6 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-TWN-FSRV1-Home -Members $textbox1.text
}

} 





