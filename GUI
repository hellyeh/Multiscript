Add-ADGroupMember -Identity GLS-U-KAL-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Launceston")
{

Add-ADGroupMember -Identity fc2dbcac-9868-4a1a-9b12-a4c954889b9c -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-LAU-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Mackay")
{

Add-ADGroupMember -Identity e138a0f5-a934-4cba-b8d0-d9c9a129401c -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-MCK-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Melbourne")
{

Add-ADGroupMember -Identity dc5104c3-9268-4829-8313-fbb7e0e3c51e -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-MEL-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Muswellbrook")
{

Add-ADGroupMember -Identity 5757a7fd-2668-4633-9fbd-cdaeaa2a28e7 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-MUS-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Newcastle")
{

Add-ADGroupMember -Identity 7e78c631-240a-4144-a6ca-b9f830379de5 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-NEW-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Perth")
{

Add-ADGroupMember -Identity 89f37684-cd78-43b6-b547-cc25cde2f122 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-PER-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Port Hedland")
{

Add-ADGroupMember -Identity 50b6b502-430e-4956-8a43-9bea87f8243f -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-PER-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Somerton")
{

Add-ADGroupMember -Identity 23f1179a-29ea-4524-a7d6-bebf558d28d7 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-SOM-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Revesby")
{

