function Get-HCABitlocker ($RecovInput)
{
    
    Set-ExecutionPolicy -Scope Process -ExecutionPolicy Bypass

#cd C:\users\a-lay\Documents

import-module C:\users\a-lay\Documents\Get-BDERecoveryPassword.ps1 -force

#$RecovInput = read-host -Prompt "
    #Enter 8 character recovery ID eg. B16DCDD8 | OR |  Computername eg. HIT-PC02LRTC "
if ($Recovinput.Length -eq 8)
{
    Get-BDERecoveryPassword -RecoveryGUID $RecovInput |select RecoveryPassword
}

#if ($Recovinput.Length -gt 8)
#{
#    "
#    Recovery ID entered is MORE THAN 8 chars"
#}

if ($Recovinput.Length -lt 8)
{
    "
    Recovery ID entered is LESS THAN 8 chars"
}

if ($Recovinput.StartsWith("hit-") -or $Recovinput.StartsWith("HIT-") )
{
    Get-BDERecoveryPassword -ComputerName $RecovInput |select RecoveryPassword
}

}


function Get-HCAou ()
{

import-Module ActiveDirectory
#$Branches = import-csv C:\users\a-edwardsbr\documents\Branches.csv


$y = Get-ADUser $textbox1.text | select distinguishedname 

if ($y -match "OU=(?<content>.*),OU=Managed Users,OU=HCMA,DC=hcma,DC=com,DC=au")
{
"This Users current OU is " + $matches['content']
}

}

function Get-HCAEN ()

{

$x = get-aduser $TextBox1.Text -Properties employeenumber | select employeenumber

$textbox4.text = $x.employeenumber

}

function Get-HCALO ()

{

$lo = get-aduser $TextBox1.Text -Properties lockedout | select lockedout

$label2.text = "Current lockout status: " + $lo.lockedout 



}

function Get-HCAAE ()

{

$AE = get-aduser $TextBox1.Text -Properties AccountExpirationDate | select AccountExpirationDate

$label6.text = "Account Expiration Date: " + $AE.AccountExpirationDate 



}

function Get-HCAPE ()

{

$PE = get-aduser $TextBox1.Text -Properties PasswordExpired | select PasswordExpired

$label7.text = "Password Expirey Status: " + $PE.PasswordExpired 



}

function Get-HCACreated ()

{

$CR = get-aduser $TextBox1.Text -Properties created | select created

$label8.text = "Account creation date: " + $CR.created 



}



function Move-HCASite ($dropdown)
{




if ($DropDown -eq "Greystanes")
{

Add-ADGroupMember -Identity 7c377404-9b9c-40e6-a2ac-95ec76b95870 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-SYDFPRS1-Home -Members $textbox1.text
}
if ($DropDown -eq "Adelaide")
{

Add-ADGroupMember -Identity 01728730-a4c1-4de2-bd0d-0e033675324a -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-ADL-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Albury")
{

Add-ADGroupMember -Identity 6a02546e-222a-4ba9-a1ba-49bec2fa6455 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-ALB-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Brisbane")
{

Add-ADGroupMember -Identity a310faea-1d35-4e3d-8baa-7190e0df93ac -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-BRI-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Bunbury")
{

Add-ADGroupMember -Identity 6d3fd786-5faa-477a-bb5e-709398c618c8 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-BUN-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Canberra")
{

Add-ADGroupMember -Identity e7617829-f173-496f-87b8-063a2046aa8f -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-REV-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Darwin")
{

Add-ADGroupMember -Identity 119a2082-a95b-4ede-b77e-6c38fdb937bb -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-DAR-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Emerald")
{

Add-ADGroupMember -Identity fff18d5f-a817-476c-930a-e364a2c44ca9 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-EML-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Gunnedah")
{

Add-ADGroupMember -Identity 32334ddb-1c9a-434a-8fd3-67db01c707b4 -members $textbox1.text
Add-ADGroupMember -Identity GLS-U-MUS-FSRV1-Home -Members $textbox1.text
}
if ($DropDown -eq "Kalgoorlie")
{

